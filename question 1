def bin_search(listA, val):
    start = 0
    end = len(listA) - 1
    
    while start <= end:
        mid = (start + end) // 2  
        if listA[mid] == val:
            return mid 
        elif listA[mid] < val:
            start = mid + 1  
        else:
            end = mid - 1 
    
    return -1  

nums = [1, 3, 5, 7, 9, 11, 13]
x = 7

pos = bin_search(nums, x)
if pos != -1:
    print("found at", pos)
else:
    print("not found")
